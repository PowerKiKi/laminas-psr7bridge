{
    "name": "zendframework/zend-psr7bridge",
    "description": "PSR-7 <-> Zend\\Http bridge",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "http",
        "psr",
        "psr-7"
    ],
    "homepage": "https://github.com/zendframework/zend-psr7bridge",
    "support": {
        "issues": "https://github.com/zendframework/zend-psr7bridge/issues",
        "source": "https://github.com/zendframework/zend-psr7bridge"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zend-http": "^2.5",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Psr7Bridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Psr7Bridge\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
